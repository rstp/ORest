%a(href="/newclient") Ajoute un Contact
%table
  %tr
    %th{:width=>"100px;"}
    %th{:width=>"100px;"}
    %th{:width=>"300px;"}
  - cs.each do|c|
    %tr
      %td= c['nom']
      %td= c['tele']
      %td= em(c['CID'])
      %td
        %a.red(href="/client/#{c['CID']}") Voir
      %td
        %a.blue(href="/client/#{c['CID']}/edit") Edit
